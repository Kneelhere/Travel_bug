	<h1 class="text-center"> San Francisco </h1>
    <form id="new-place-form" class="text-center" method="post" action="/sf">
    <div>
      <input type="text" name="place[name]" placeholder="Name">
    </div><br>
    <div>
      <textarea type="text" name="place[description]" placeholder="Description"></textarea>
    </div><br>
    <button type="submit">Submit</button>
  </form>
  <ul class="list-group" id="place-ul"></ul>
    <script type="text/template" id="places-template">
    <li class='list-group-item'><%= name %>
      <!-- Places label -->
      <span class="label label-default"><%= description%></span>
    </li>
  </script>
